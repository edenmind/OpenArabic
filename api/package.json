{
  "name": "backend",
  "version": "0.9.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.js",
  "directories": {
    "tests": "tests"
  },
  "scripts": {
    "test": "API_KEY='somesecurekey' tap \"tests/**/*.test.js\" --branches 20 --functions 50 --statements 70 --lines 70 --coverage-report=cobertura",
    "test-local": "./tests/mongo-helper.sh -s && API_KEY='somesecurekey' tap \"tests/**/*.test.js\" --branches 20 --functions 50 --statements 70 --lines 70 --coverage-report=cobertura && ./tests/mongo-helper.sh -t",
    "start": "fastify start -l info app.js",
    "dev": "fastify start -w -l info -P app.js",
    "lint": "eslint '**/*.js' --ignore-pattern node_modules/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.4.1",
    "@fastify/cors": "^8.1.1",
    "@fastify/env": "^4.1.0",
    "@fastify/helmet": "^10.0.2",
    "@fastify/mongodb": "^6.1.0",
    "@fastify/rate-limit": "^7.5.0",
    "@fastify/sensible": "^5.1.1",
    "@fastify/static": "^6.5.0",
    "@fastify/swagger": "^8.1.0",
    "axios": "^1.1.3",
    "dotenv": "^16.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-putout": "^16.3.0",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-unicorn": "^44.0.2",
    "fastify": "^4.9.2",
    "fastify-cli": "^5.5.1",
    "fastify-healthcheck": "^4.2.0",
    "fastify-plugin": "^4.3.0",
    "moment": "^2.29.4",
    "mongodb": "^4.11.0",
    "prettier": "^2.7.1",
    "putout": "^28.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "eslint": "^8.26.0",
    "tap": "^16.3.0"
  }
}
